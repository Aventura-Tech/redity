{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "noImplicitThis": false,
    "alwaysStrict": true,
    "types": [
      "node",
      "jest"
    ],
    "esModuleInterop": true,
    "jsx": "react",
    "resolveJsonModule": true
  },
  "include": [
    "test",
    "src",
    "src/Pages/model",
    "setupTests.js"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "src/Pages"
  ],
  "resolveJsonModule": true
}
